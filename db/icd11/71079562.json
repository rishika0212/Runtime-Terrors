  "title": {
    "@language": "en",
    "@value": "Extrathoracic coccidioidomycosis"
  },
  "definition": {
    "@language": "en",
    "@value": "Coccidioidomycosis involving sites other than the lungs and thoracic cavity. Recognised sites include lymph nodes, bones, joints, central nervous system and skin. Transmission is through haematogenous spread to other body sites after inhalation of fungal spores or by direct inoculation."
  },
  "longDefinition": {
    "@language": "en",
    "@value": "Extrapulmonary infection is clinically detected outside the thoracic cavity in fewer than 1% of all cases. This is associated with a poorer outcome and usually requires prolonged treatment with antifungal agents."
  },
