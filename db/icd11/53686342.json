  "title": {
    "@language": "en",
    "@value": "Acute kidney failure, stage 3"
  },
  "definition": {
    "@language": "en",
    "@value": "Rate of change of serum creatinine: 3.0 times baseline OR increase to 4.0 mg/dl OR need for renal replacement therapy (i.e. dialysis) or for patients <18 years, decrease in GFR to <35 ml/min per 1.73 m2 OR Magnitude of urine output: <0.3 ml/kg/h for 24 hours OR anuria for >= 12 hours"
  },
  "longDefinition": {
    "@language": "en",
    "@value": "Rate of change of serum creatinine: 3.0 times baseline OR increase to 4.0 mg/dl OR need for renal replacement therapy (ie dialysis) or for patients <18 years, decrease in GFR to <35 ml/min per 1.73 m2 OR Magnitude of urine output: <0.3 ml/kg/h for 24 hours OR anuria for >= 12 hours"
  },
