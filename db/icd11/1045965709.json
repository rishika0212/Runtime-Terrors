  "title": {
    "@language": "en",
    "@value": "Secondary progressive multiple sclerosis"
  },
  "longDefinition": {
    "@language": "en",
    "@value": "To date, there are no clear clinical, imaging, immunological or pathological criteria to determine the transition point when relapsing remitting multiple sclerosis converts to secondary progressive multiple sclerosis; the transition is usually gradual. This has limited our ability to study the imaging and biomarker characteristics that may distinguish this course."
  },
  "relatedEntitiesInMaternalChapter": [
    "http://id.who.int/icd/entity/564191997"
  ],
  "codingNote": {
    "@language": "en",
    "@value": "This category is to be used to indicate Secondary progressive multiple sclerosis, after an initially relapsing/remitting course (includes remitting relapsing progressive, may have superimposed relapses)"
  },
