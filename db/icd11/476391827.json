  "title": {
    "@language": "en",
    "@value": "Acute kidney failure"
  },
  "definition": {
    "@language": "en",
    "@value": "An increase in serum creatinine by 0.3 mg/dl or greater within 48 hours; or increase in serum creatinine by 1.5-fold or greater above baseline, which is known or presumed to have occurred within 7 days; or urine volume less than 0.5 ml/kg/h for 6 hours or more."
  },
  "inclusion": [
    {
      "label": {
        "@language": "en",
        "@value": "nontraumatic acute kidney injury"
      }
    }
  ],
