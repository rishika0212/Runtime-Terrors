  "title": {
    "@language": "en",
    "@value": "Haemorrhagic disorder due to circulating anticoagulants and coagulation factors"
  },
  "definition": {
    "@language": "en",
    "@value": "A disease caused by anticoagulants present in the body that prevent the blood from clotting normally. This disease is characterised by abnormalities in blood clotting. This disease may present with prolonged bleeding, easy bruising or bleeding gums. Confirmation is by identification of anticoagulants present in a blood sample."
  },
  "exclusion": [
    {
      "label": {
        "@language": "en",
        "@value": "long-term use of anticoagulants without haemorrhage"
      },
