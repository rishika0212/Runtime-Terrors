  "title": {
    "@language": "en",
    "@value": "Tetanus neonatorum"
  },
  "definition": {
    "@language": "en",
    "@value": "A disease affecting neonates, caused by an infection with the gram-positive bacteria Clostridium tetani. This disease is characterised by systemic muscle spasms that arise within the first few days after delivery. Transmission is commonly by direct contact or lack of maternal immunity."
  },
  "longDefinition": {
    "@language": "en",
    "@value": "A disease characterized by systemic muscle spasms arising in newborns who are delivered in unsanitary conditions and subsequently infected with Clostridium tetani."
  },
