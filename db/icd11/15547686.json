  "title": {
    "@language": "en",
    "@value": "Fetus or newborn affected by noxious influences transmitted via placenta or breast milk"
  },
  "definition": {
    "@language": "en",
    "@value": "A group of conditions characterised by findings in the fetus or newborn due to the transmission of any harmful or poisonous substance to the fetus or newborn via the placenta or in breast milk."
  },
  "inclusion": [
    {
      "label": {
        "@language": "en",
        "@value": "nonteratogenic effects of substances transmitted via placenta"
      }
    }
  ],
  "exclusion": [
    {
      "label": {
        "@language": "en",
        "@value": "congenital malformations"
      },
      "foundationReference": "http://id.who.int/icd/entity/223744320",
      "linearizationReference": "http://id.who.int/icd/release/11/2024-01/mms/223744320"
    },
